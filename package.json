{
  "name": "document-ms",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "babel": "babel --presets es2015 --ignore node_modules",
    "babel-node": "babel-node --presets es2015 --ignore node_modules",
    "build": "npm run clean && npm run build:prod && npm run build:node",
    "build:dev": "NODE_ENV=development webpack --progress --colors --config ./webpack/webpack.config.dev.js",
    "build:node": "NODE_ENV=production npm run babel -- ./server -d ./compiled",
    "build:prod": "NODE_ENV=production webpack --progress --colors --config ./webpack/webpack.config.prod.js",
    "clean": "rimraf compiled",
    "coverage": "cat ./test/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "dev": "NODE_ENV=development nodemon",
    "postinstall": "npm run build",
    "posttest": "npm run coverage",
    "start": "NODE_ENV=production node compiled/index.js",
    "test": "istanbul cover --dir ./test/coverage ./node_modules/.bin/_mocha -- -u exports --compilers js:babel-core/register --require ./test/setup.js \"test/**/*.spec@(.js|.jsx)\" ",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.13.2",
    "dotenv": "^2.0.0",
    "draft-js": "^0.8.1",
    "express": "~4.13.1",
    "jsonwebtoken": "^6.2.0",
    "jwt-decode": "^2.1.0",
    "material-ui": "^0.15.4",
    "mongoose": "^4.4.14",
    "nodemon": "^1.10.2",
    "passport": "^0.3.2",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "rimraf": "^2.5.4",
    "socket.io": "^1.4.8",
    "superagent": "^2.2.0",
    "supertest": "^1.2.0",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-istanbul": "^0.8.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.0",
    "chai-immutable": "^1.6.0",
    "coveralls": "^2.11.9",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^9.4.2",
    "mocha": "^2.4.5",
    "nock": "^8.0.0",
    "should": "^8.3.1",
    "supertest": "^1.2.0"
  },
  "engines": {
    "node": "6.3.0",
    "npm": "3.10.3"
  }
}
