{
  "extends": "airbnb",
  "rules": {
    "arrow-body-style": 0,
    "new-cap": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "comma-dangle": 0,
    "no-shadow": 0,
    "import/no-extraneous-dependencies": 0,
    "no-lone-blocks": 0,
    "consistent-return": 0,
    "func-names": 0,
    "no-unused-expressions": 0,
    "no-case-declarations": 0
  },
  "env": {
    "browser": true,
    "mocha": true
  }
}
